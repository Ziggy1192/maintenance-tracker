<div class="container">
  <%= simple_form_for @task do |f| %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :extra_info %>
    <%= f.association :service %>
    <%= f.association :technician, label_method: :first_name %>
    <%= f.association :technician, label_method: :last_name %>
    <%= f.button :submit %>
  <% end %>
</div>
